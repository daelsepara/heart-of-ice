#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|LEXBUF|GETB|CONTBIT|OPENBIT|NARTICLEBIT|PLURALBIT|PERSONBIT|TOUCHBIT|PRINTN|GIVEBAG|PARADOXING|HUMBABA|PRACTICED|SHORTSWORD|ENKIDU|URUK|TALOS|: CT$|OBJ$|: Du$|: En$" | grep -i -v -Ev "defense|realization|mesmerized|recognize|realize|shortsword|hoves|hubble|machina|quayside|haloes|psionic|psychokinetic|agonized|relight|sycophancy|characterizes|plastiwood|viewscreen|cordis" | grep -i -v -Ev ": barysal$|: volent$|: saharan$|: apennine$|: caretek$|: careteks$|: ligurian$|: volentine$|: venis$|: kahira$|: kahira's$|: lyonesse$|: boche$|: marsay$|: humbaba$|: manta$|: gondo$|: kashira$|: pinar$|: burrek$|: copt$|: novak$|: golgoth$|: gargan$|: mantramukta$|: speculum$|: hoves$|: burrek's$|: vajra$|: boche$|: boche's$|: siriasis$|: bezant$|: jaffe$|: bador$" | uniq

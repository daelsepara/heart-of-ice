#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|LEXBUF|GETB|CONTBIT|OPENBIT|NARTICLEBIT|PLURALBIT|PERSONBIT|TOUCHBIT|PRINTN|GIVEBAG|PARADOXING|HUMBABA|: CT$|OBJ$|: Du$|: En$" | grep -i -v -Ev "defense|realization|mesmerized|recognize|realize|barysal$|volent$|saharan$|apennine$|caretek$|careteks$|ligurian$|volentine$|venis$|kahira$|lyonesse$|boche$|marsay$|humbaba$|manta$|gondo$" | uniq

#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|LEXBUF|GETB|CONTBIT|OPENBIT|NARTICLEBIT|PLURALBIT|PERSONBIT|TOUCHBIT|PRINTN|GIVEBAG|: CT$|: EB$|OBJ$" | grep -i -v -Ev "" | uniq

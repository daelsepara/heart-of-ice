#!/bin/bash
spell -n story.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|LEXBUF|GETB|CONTBIT|OPENBIT|NARTICLEBIT|PLURALBIT|PERSONBIT|TOUCHBIT|PRINTN|GIVEBAG|PARADOXING|HUMBABA|PRACTICED|SHORTSWORD|: CT$|OBJ$|: Du$|: En$" | grep -i -v -Ev "defense|realization|mesmerized|recognize|realize|shortsword|hoves|barysal$|volent$|saharan$|apennine$|caretek$|careteks$|ligurian$|volentine$|venis$|kahira$|kahira's$|lyonesse$|boche$|marsay$|humbaba$|manta$|gondo$|kashira$" | uniq
